dontaudit init { bt_firmware_file firmware_file }:filesystem getattr;

allow init self:netlink_route_socket rw_socket_perms;
allow init self:rawip_socket create_socket_perms;
allow init self:socket { ioctl create write read };
allow init sysfs_wake_lock:file { read write };
allow init self:capability { dac_override };
allow init netutils_wrapper_exec:file { execute execute_no_trans getattr open read };
